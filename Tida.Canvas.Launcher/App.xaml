<Application x:Class="Tida.Canvas.Launcher.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:app="clr-namespace:Tida.Application.App;assembly=Tida.Application">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                

                <app:ResourceDictionaryEx  Name="{x:Static app:Constants.LanguageDict}">
                    <ResourceDictionary.MergedDictionaries>
                        <ResourceDictionary Source="pack://application:,,,/Tida.Canvas.Shell;component/Shell/Languages/zh_CN/Tida.Canvas.Shell.Shell.xaml" />
                        <ResourceDictionary Source="pack://application:,,,/Tida.Application;component/App/Languages/zh_CN/Tida.Application.App.xaml" />
                        <!--<ResourceDictionary Source="pack://application:,,,/Tida.Canvas.Shell;component/EditTools/Languages/zh_CN/Tida.Canvas.Shell.EditTools.xaml" />-->
                        <ResourceDictionary Source="pack://application:,,,/Tida.Canvas.Shell;component/Canvas/Languages/zh_CN/Tida.Canvas.Shell.Canvas.xaml" />
                    </ResourceDictionary.MergedDictionaries>
                </app:ResourceDictionaryEx>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
